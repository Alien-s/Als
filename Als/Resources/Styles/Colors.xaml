    <!--Colors of the Buttons-->
    <SolidColorBrush x:Key="Button.Static.Background" Color="#FFCC66"/>
    <SolidColorBrush x:Key="Button.Static.Border" Color="#CC8800"/>
    <SolidColorBrush x:Key="Button.Static.Foreground" Color="#000000"/>

    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFFF00"/>
    <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#CCCC00"/>

    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#EEEE88"/>
    <SolidColorBrush x:Key="Button.Pressed.Border" Color="#999900"/>

    <SolidColorBrush x:Key="Button.Disabled.Background" Color="#F4F4F4"/>
    <SolidColorBrush x:Key="Button.Disabled.Border" Color="#ADB2B5"/>
    <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#838383"/>
    
    
